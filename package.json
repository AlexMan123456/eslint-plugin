{
  "name": "@alextheman/eslint-plugin",
  "version": "1.3.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest",
    "format": "npm run build && prettier --write --parser typescript 'src/**/*.ts' 'tests/**/*.ts' && ESLINT_MODE=fix eslint --fix 'src/**/*.ts' 'tests/**/*.ts'",
    "lint": "npm run build && ESLINT_MODE=lint eslint 'src/**/*.ts' 'tests/**/*.ts' && prettier --check --parser typescript 'src/**/*.ts' 'tests/**/*.ts'",
    "update-dependencies": "npx npm-check-updates -u && npm install",
    "prepare": "husky",
    "build": "tsup",
    "change-major": "npm version major -m \"Change version number to v%s\"",
    "change-minor": "npm version minor -m \"Change version number to v%s\"",
    "change-patch": "npm version patch -m \"Change version number to v%s\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=8.37.0",
    "@typescript-eslint/parser": ">=8.37.0",
    "eslint": ">=9.31.0",
    "eslint-config-prettier": ">=10.1.8",
    "eslint-import-resolver-typescript": ">=4.4.4",
    "eslint-plugin-import": ">=2.32.0",
    "eslint-plugin-prettier": ">=5.5.3",
    "eslint-plugin-react": ">=7.37.5",
    "eslint-plugin-react-hooks": ">=5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-react-refresh": ">=0.4.20"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/rule-tester": "^8.39.1",
    "@typescript-eslint/utils": "^8.39.1",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@alextheman/utility": "^1.11.2"
  }
}
